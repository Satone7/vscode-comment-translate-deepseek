{
  "name": "deepseek-comment-translate",
  "displayName": "deepseek-comment-translate",
  "description": "deepseek translate source for comment-translate",
  "version": "1.0.2",
  "publisher": "renping",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translateSource",
    "comment translate",
    "deepseek",
    "deepseek translate",
    "翻译",
    "注释"
  ],
  "homepage": "https://github.com/Satone7/vscode-comment-translate-deepseek#readme",
  "bugs": {
    "url": "https://github.com/Satone7/vscode-comment-translate-deepseek/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Satone7/vscode-comment-translate-deepseek.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "image/logo.png",
  "extensionDependencies": [
    "intellsmi.comment-translate"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "deepseek translate Configuration",
        "type": "Object",
        "properties": {
          "deepseekTranslate.authKey": {
            "type": "string",
            "markdownDescription": "You need an authentication key to access to the API.[Accessing the API](https://platform.deepseek.com/api_keys)"
          },
          "deepseekTranslate.apiType": {
            "type": "string",
            "enum": [
              "openai",
              "ollama"
            ],
            "default": "openai",
            "description": "API类型选择，支持OpenAI兼容接口或本地Ollama服务"
          },
          "deepseekTranslate.apiBaseUrl": {
            "type": "string",
            "default": "https://api.deepseek.com",
            "description": "API基础地址，本地部署时可改为http://localhost:11434"
          },
          "deepseekTranslate.model": {
            "type": "string",
            "default": "deepseek-chat",
            "description": "使用的模型名称，Ollama部署时填写本地模型名称"
          }
        }
      }
    ],
    "translates": [
      {
        "translate": "deepseek",
        "title": "deepseek translate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "0.21.4",
    "comment-translate-manager": "^0.0.2",
    "querystring": "^0.2.1"
  }
}
